{
  "name": "@happycastle/channel-talk",
  "version": "0.2.0",
  "description": "OpenClaw Channel Talk (채널톡) Team Chat channel plugin",
  "type": "module",
  "license": "MIT",
  "author": "happycastle114",
  "homepage": "https://github.com/happycastle114/openclaw-channel-talk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/happycastle114/openclaw-channel-talk.git"
  },
  "keywords": [
    "openclaw",
    "channel-talk",
    "채널톡",
    "chatbot",
    "team-chat",
    "plugin"
  ],
  "files": [
    "index.ts",
    "src/",
    "openclaw.plugin.json",
    "README.md"
  ],
  "dependencies": {
    "express": "^4.19.0"
  },
  "peerDependencies": {
    "openclaw": "*"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "channel-talk",
      "label": "Channel Talk",
      "selectionLabel": "Channel Talk (채널톡)",
      "docsPath": "channel-talk",
      "docsLabel": "Channel Talk Setup",
      "blurb": "채널톡 Team Chat integration",
      "aliases": [
        "channeltalk",
        "채널톡"
      ],
      "order": 110
    },
    "install": {
      "npmSpec": "@happycastle/channel-talk",
      "localPath": "extensions/channel-talk",
      "defaultChoice": false
    },
    "pluginId": "channel-talk"
  }
}
